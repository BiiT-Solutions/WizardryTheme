{
  "name": "storybook-biit",
  "version": "1.1.80",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-library": "npm run transloco:extract && ng build biit-ui",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "transloco:clean": "del-cli --force ./src/assets/i18n/biit-ui",
    "transloco:extract": "npm run transloco:clean && transloco-scoped-libs",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006 --no-version-updates",
    "build-storybook": "npm run transloco:extract && npm run docs:json && build-storybook",
    "prepare-publication": "cp ./README.md ./storybook-static/ && echo \"{\\\"name\\\": \\\"`node -e \"console.log(require('./package.json').name);\"\n`\\\", \\\"version\\\": \\\"`node -e \"console.log(require('./package.json').version);\"\n`\\\"}\" > ./storybook-static/package.json",
    "prepare-translations": "cpx './src/assets/i18n/biit-ui/**/*.json' './dist/biit-ui/assets/i18n/'",
    "prepare-translations-local": "cpx './src/assets/i18n/biit-ui/**/*.json' './dist/biit-ui/assets/i18n/'",
    "generate-zip": "mkdir -p ./dist-zip && tar -zcvf ./dist-zip/storybook.gz ./storybook-static",
    "publish-storybook": "npm run build-storybook && npm run prepare-publication && npm publish ./storybook-static",
    "publish-library": "npm run build-library && npm run prepare-translations && npm publish ./dist/biit-ui/"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/material-moment-adapter": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "apexcharts": "^3.37.1",
    "biit-icons-collection": "latest",
    "material-design-icons": "^3.0.1",
    "ng-apexcharts": "^1.7.4",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "~15.0.5",
    "@angular/compiler-cli": "^15.0.0",
    "@babel/core": "^7.20.12",
    "@compodoc/compodoc": "^1.1.19",
    "@ngneat/transloco": "^4.2.2",
    "@ngneat/transloco-scoped-libs": "^3.0.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/angular": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/jasmine": "~4.3.0",
    "@types/uuid": "^9.0.2",
    "babel-loader": "^8.3.0",
    "del-cli": "^5.0.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^15.0.0",
    "typescript": "~4.8.2",
    "cpx": "^1.5.0"
  }
}
